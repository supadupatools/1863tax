{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/styles.v2.css",
      "destination": "/styles.v2.css"
    },
    {
      "source": "/src/public/images/:path*",
      "destination": "/src/public/images/:path*"
    },
    {
      "source": "/login",
      "destination": "/web/admin/login.html"
    },
    {
      "source": "/login/",
      "destination": "/web/admin/login.html"
    },
    {
      "source": "/admin",
      "destination": "/web/admin/index.html"
    },
    {
      "source": "/admin/",
      "destination": "/web/admin/index.html"
    },
    {
      "source": "/admin/login",
      "destination": "/web/admin/login.html"
    },
    {
      "source": "/admin/login/",
      "destination": "/web/admin/login.html"
    },
    {
      "source": "/admin/:path*",
      "destination": "/web/admin/:path*"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/search",
      "destination": "/web/public/index.html"
    },
    {
      "source": "/search/",
      "destination": "/web/public/index.html"
    },
    {
      "source": "/search/:path*",
      "destination": "/web/public/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/web/public/:path*"
    }
  ]
}
