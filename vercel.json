{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/admin",
      "destination": "/web/admin/index.html"
    },
    {
      "source": "/admin/",
      "destination": "/web/admin/index.html"
    },
    {
      "source": "/admin/login",
      "destination": "/web/admin/login.html"
    },
    {
      "source": "/admin/login/",
      "destination": "/web/admin/login.html"
    },
    {
      "source": "/admin/:path*",
      "destination": "/web/admin/:path*"
    },
    {
      "source": "/",
      "destination": "/web/public/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/web/public/:path*"
    }
  ]
}
